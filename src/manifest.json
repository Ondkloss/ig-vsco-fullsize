{
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "version": "1.2.1",
    "manifest_version": 2,
    "default_locale": "en",
    "permissions": [
        "activeTab",
        "https://www.instagram.com/*",
        "https://vsco.co/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://vsco.co/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {},
    "icons": {
        "16": "images/logo16.png",
        "32": "images/logo32.png",
        "48": "images/logo48.png",
        "128": "images/logo128.png"
    }
}